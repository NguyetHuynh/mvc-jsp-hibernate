<?xml version="1.0"?>


<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
              
              <hibernate-mapping>
              
                <class name="spring.Stock" table="Stock" catalog="mykongdb">
                
                    <id name="id" column="stock_ID" type="int">
                        <generator class="increment"/>
                    </id>
                    
                    <property name="categoryId" column="CategoryId" type="int"/>
              		<property name="stockCode" column="stock_code" type="string"/>
              		<property name="stockName" column="stock_name" type="string"/>
                        <property name="stockDesc" column="stock_desc" type="string"/>
                </class>
                
              </hibernate-mapping>
